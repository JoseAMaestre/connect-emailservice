{
  "name": "Email Service",
  "description": "Sends an email when a new fulfillment order arrives",
  "version": "0.1.0",
  "readme_url": "https://example.com/README.md",
  "changelog_url": "https://example.com/CHANGELOG.md",
  "capabilities": {
    "asset_purchase_request_processing": [
      "draft",
      "tiers_setup",
      "pending",
      "inquiring",
      "approved",
      "failed",
      "scheduled",
      "revoking",
      "revoked"
    ]
  },
  "variables": [
    {
      "name": "HTMLBODY",
      "secure": false
    },
    {
      "name": "USERNAME",
      "secure": false
    },
    {
      "name": "PASSWORD",
      "secure": true
    },
    {
      "name": "MAILFROM",
      "secure": false
    },
    {
      "name": "MAILSUBJECT",
      "secure": false
    },
    {
      "name": "SMTPHOST",
      "secure": false
    }
  ]
}